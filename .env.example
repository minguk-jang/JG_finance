# Supabase Configuration
# Get these from your Supabase project settings (https://app.supabase.com)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Gemini AI (optional - for Quick Add Voice feature)
VITE_GEMINI_API_KEY=your-gemini-api-key-here

# Calendar Configuration (optional)
# Default timezone for calendar events (supported: Asia/Seoul, UTC, etc.)
VITE_CALENDAR_TIMEZONE=Asia/Seoul

# Default calendar color palette (supported: sky, coral, orange, tan, amber, dark)
VITE_CALENDAR_DEFAULT_PALETTE=sky

# Default reminders in minutes before event (comma-separated)
VITE_CALENDAR_DEFAULT_REMINDERS=15,60
